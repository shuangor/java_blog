<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.winterchen.dao.ContentDao">
    <insert id="insert" parameterType="com.winterchen.model.ContentDomain" >
        insert into t_content (tap,content) values (#{tap},#{content})
    </insert>
    <select id="allContent" resultType="com.winterchen.model.ContentDomain" >
        select * from t_content
    </select>

    <select id="byIdContent" >

    </select>
</mapper>